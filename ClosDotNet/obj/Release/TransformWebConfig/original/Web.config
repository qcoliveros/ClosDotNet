  </runtime>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1211">
        <arg key="configType" value="FILE-WATCH"/>
        <arg key="configFile" value="~/Config/log4net.config"/>
      </factoryAdapter>
    </logging>
  </common>
  <spring>
    <parsers>
      <parser type="Spring.Data.Config.DatabaseNamespaceParser, Spring.Data"/>
      <parser type="Spring.Transaction.Config.TxNamespaceParser, Spring.Data"/>
      <parser type="Spring.Aop.Config.AopNamespaceParser, Spring.Aop"/>
    </parsers>
    <context>
      <resource uri="file://~/Config/Spring/AppContext_BO.xml"/>
      <resource uri="file://~/Config/Spring/AppContext_Configuration.xml"/>
      <resource uri="file://~/Config/Spring/AppContext_Controller.xml"/>
      <resource uri="file://~/Config/Spring/AppContext_Mapper.xml"/>
